{
  "name": "abcjs-vscode",
  "displayName": "abcjs vscode",
  "description": "Abc music notation editor with abcjs",
  "version": "1.0.0",
  "publisher": "alensiljak",
  "author": {
    "name": "Alen Å iljak"
  },
  "license": "GPL-3.0-or-later",
  "icon": "res/icon128.png",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/alensiljak/abcjs-vscode.git"
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:abcjs-vscode.helloWorld",
    "onCommand:abcjs-vscode.showPreview"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "abcjs-vscode.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "abcjs-vscode.showPreview",
        "title": "abcjs-vscode: Show Preview"
      }
    ],
    "languages": [
      {
        "id": "abc",
        "aliases": [
          "ABC"
        ],
        "extensions": [
          ".abc"
        ]
      }
    ],
    "grammars": [
      {
        "language": "abc",
        "scopeName": "source.abc",
        "path": "./abc.tmGrammar.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.55.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "eslint": "^7.22.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "typescript": "^4.2.4",
    "vscode-test": "^1.5.2"
  }
}
